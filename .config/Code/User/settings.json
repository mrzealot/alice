{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.mouseWheelZoom": true,
    "workbench.activityBar.visible": false,
    "breadcrumbs.enabled": false,
    "diffEditor.renderSideBySide": true,
    "terminal.integrated.rendererType": "dom",
    "editor.multiCursorModifier": "alt",
    "explorer.confirmDragAndDrop": false,
    "latex-workshop.view.pdf.viewer": "browser",
    "explorer.confirmDelete": false,
    "cSpell.checkLimit": 5000,
    "cSpell.maxNumberOfProblems": 1000,
    "cSpell.userWords": [
        "else's",
        "misperceptions"
    ],
    "git.confirmSync": false,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "less",
        "php",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "editor.fontFamily": "'Hack', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "workbench.editor.showTabs": true,
    "spellright.configurationScope": "user",
    "spellright.language": [
        "en_US",
        "hu_HU"
    ],
    "spellright.documentTypes": [
        "latex",
        "plaintext",
        "markdown"
    ],
    "spellright.notificationClass": "information",
    "editor.minimap.enabled": true,
    "wordcounter.count_chars": false,
    "wordcounter.count_paragraphs": false,
    "wordcounter.readtime": false,
    "wordcounter.count_lines": false,
    "diffEditor.maxComputationTime": 0,
    "wordcounter.side.left": [
        "word"
    ],
    "diffEditor.ignoreTrimWhitespace": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "extensions.ignoreRecommendations": true,
    "window.menuBarVisibility": "toggle",
    "latex-workshop.latex.autoBuild.run": "never",
    "window.zoomLevel": 1
}